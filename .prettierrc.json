{
  "plugins": ["prettier-plugin-svelte"],
  "overrides": [{ "files": "*.svelte", "options": { "parser": "svelte" } }],
  "singleQuote": false,
  "semi": true,
  "trailingComma": "none",
  "bracketSpacing": true,
  "bracketSameLine": false,
  "endOfLine": "lf",
  "tabWidth": 2,
  "printWidth": 120
}
